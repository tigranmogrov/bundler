{
    "name": "bundler",
    "version": "1.0.0",
    "scripts": {
        "dev": "webpack serve -c config/dev.js",
        "build": "webpack -c config/build.js",
        "prod": "webpack -c config/prod.js",
        "watch": "webpack --watch -c config/prod.js",
        "lint": "eslint .",
        "lint-fix": "eslint . --fix"
    },
    "browserslist": [
        "last 3 version",
        "> 1%",
        "not IE >= 0"
    ],
    "sideEffects": [
        "*.js",
        "*.css",
        "*.scss",
        "*.html"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tigranmogrov/bundler.git"
    },
    "author": "Tigran Mogrov",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/tigranmogrov/bundler/issues"
    },
    "devDependencies": {
        "@babel/core": "^7.18.6",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.18.6",
        "babel-loader": "^8.2.5",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.7.1",
        "css-minimizer-webpack-plugin": "^4.0.0",
        "eslint": "^8.19.0",
        "fs": "^0.0.1-security",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.6.1",
        "path": "^0.12.7",
        "postcss-import": "^14.1.0",
        "postcss-loader": "^7.0.0",
        "postcss-normalize-charset": "^5.1.0",
        "postcss-preset-env": "^7.7.2",
        "sass": "^1.53.0",
        "sass-loader": "^13.0.2",
        "terser-webpack-plugin": "^5.3.3",
        "uglify-js": "^3.16.2",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.9.3",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "include-media": "^1.4.10"
    }
}
